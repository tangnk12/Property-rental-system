      <Text layoutX="52.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Choose a user" />
      <TextField fx:id="textGiveUsername" layoutX="411.0" layoutY="134.0" />
      <Text layoutX="434.0" layoutY="126.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username Setting" wrappingWidth="104.0" />
      <Separator layoutX="172.0" layoutY="62.0" orientation="VERTICAL" prefHeight="237.0" prefWidth="0.0" />
      <Separator layoutX="396.0" layoutY="62.0" orientation="VERTICAL" prefHeight="237.0" prefWidth="0.0" />
      
   </children>
